<form method="post" action="{{ path('sanction_list_admin_organization_delete', {'id': organization.id}) }}"
      onsubmit="return confirm('Вы уверены, что хотите удалить организацию?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ organization.id) }}">
    <button class="btn btn-danger">Удалить</button>
</form>
