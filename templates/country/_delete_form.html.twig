<form method="post" action="{{ path('sanction_list_admin_country_delete', {'id': country.id}) }}"
      onsubmit="return confirm('Вы уверены, что хотите удалить страну?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ country.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash-o" aria-hidden="true"></i>&nbsp;Удалить</button>
</form>
